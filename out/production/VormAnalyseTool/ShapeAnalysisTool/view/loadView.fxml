<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="mainBox" spacing="10" alignment="TOP_LEFT" prefWidth="800" prefHeight="600" fx:controller="ShapeAnalysisTool.controller.LoadController" xmlns:fx="http://javafx.com/fxml" stylesheets="@ShapeAnalysisTool/../../style.css">
    <fx:define>
        <Font fx:id="FONT" size="18" />
    </fx:define>

    <HBox fx:id="actionBox">
        <Button fx:id="backButton" text="Back" onAction="#goBack" />
    </HBox>

    <VBox fx:id="loadBox" spacing="10" alignment="TOP_LEFT">
        <Text>Load shapes</Text>
        <Button fx:id="loadSavedShapes" text="Load saved shapes" onAction="#displayLoadedShapes"/>
        <FlowPane fx:id="loadResultPane" minWidth="560" minHeight="50">
        </FlowPane>
    </VBox>

</VBox>